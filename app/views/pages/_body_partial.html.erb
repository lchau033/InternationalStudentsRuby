<!-- /.parallax full screen background image -->
<div class="banner">
  <div class="bg-color">

    <div class="container">
      <div class="row">
        <div class="col-md-7">

          <!-- /.main title -->
          <div class="text-border">
            <h1 class="wow fadeInLeft titleNavigatingToCanada">
              Welcome to <br>
              <span class="canada">Navigating Canada</span>
              <div class="international">For International Students</div>
            </h1>
          </div>

          <!-- /.header paragraph -->
          <div class="landing-text wow fadeInUp">
            <p>Navigating Canada is a web service hoping to facilitate the process for international students as well as new comers to Canada.
              We are currently in the process of developing the service, although it will be up and running shortly!
              Register to receive release updates as well as to help us better understand our target audience. </p>
          </div>
        </div>

        <!-- /.signup form -->
        <div class="col-md-5">
          <div class="signup-header wow fadeInUp">
            <h3 class="form-title text-center">GET STARTED</h3>
            <%= form_for :user, :html => { :id => 'userform', :class=>:'form-header', :onsubmit=>'clearForm(this)'}, url: users_path do |f| %>
              <div class="form-group">
                <input class="form-control input-lg" name="user[name]" id="name" type="text" placeholder="Your name" required>
              </div>
              <div class="form-group">
                <input class="form-control input-lg" name="user[country]" id="country" type="country" placeholder="Where are you from?" required>
              </div>
              <div class="form-group">
                <input class="form-control input-lg" name="user[email]" id="email" type="email" placeholder="Email address" required>
              </div>
              <div class="form-group last">
                <input type="submit" class="btn btn-warning btn-block btn-lg" value="SUBSCRIBE" id="btnsubmit">
              </div>
              <p class="privacy text-center">We will not share your email. </p>
            <% end %>
          </div>
        </div>
      </div>
    </div>
    <a href="#coming-soon" class="mouse-hover"><div class="mouse"></div></a>
  </div>
</div>



